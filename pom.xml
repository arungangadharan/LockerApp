<project>
    <modelVersion>4.0.0</modelVersion>
    <groupId>com.locker</groupId>
    <artifactId>com.locker.LockerApp</artifactId>
    <packaging>jar</packaging>
    <version>1.0</version>
    <name>LockerApp</name>
    <url>http://maven.apache.org</url>
    
     <build>
        <sourceDirectory>src</sourceDirectory>
        <plugins>
            <plugin>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.3</version>
                <configuration>
                    <source>1.8</source>
                    <target>1.8</target>
                </configuration>
            </plugin>                 
    
        </plugins>

    </build>
</project>